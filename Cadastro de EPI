equipamentos=[]
valores=[]
certificado=[]
lote=[]
resposta="S"
while resposta=="S":
  equipamentos.append(input("Equipamento de proteção...: "))
  valores.append(float(input("Valor Uniário...: ")))
  certificado.append(int(input("CA...: ")))
  lote.append(input("Lote...: "))
  resposta=input("Responda com S para continuar: ").upper()

for equipamento in equipamentos:
  print("equipamentos ", equipamento)
